package avlnode;

/**
 *
 * @author m303user
 */
import java.util.Scanner;
 
 /* Class AVLNode */
 class AVLNode1
 {    
     AVLNode1 left, right;
     int data;
     int height;
 
     /* Constructor */
     public AVLNode1()
     {
         left = null;
         right = null;
         data = 0;
         height = 0;
     }
     /* Constructor */
     public AVLNode1(int n)
     {
         left = null;
         right = null;
         data = n;
         height = 0;
     }     
 }
